{
  "agent": {
    "default_model": {
      "provider": "ollama",
      "model": "mistral:latest"
    },
    "default_profile": "ask"
  },
  "terminal": {
    "dock": "bottom"
  },
  "auto_install_extensions": {
    "air": true,
    "catppuccin": true,
    "catppuccin icons": true,
    "csv": true,
    "discord presence": true,
    "docker-compose": true,
    "git-firefly": true,
    "dockerfile": true,
    "env": true,
    "golangci-lint": true,
    "java": true,
    "lua": true,
    "make": true,
    "meson": true,
    "nix": true,
    "python lsp": true,
    "rainbow csv": true,
    "ruby": true,
    "ruff": true,
    "sql": true,
    "templ": true,
    "toml": true,
    "xml": true,
    "zig": true,
    "svelte": true,
    "svelte snippets": true,
    "go snippets": true,
    "go sum highlighting": true,
    "python snippets": true,
    "ros√© pine": true
  },
  "autosave": "on_focus_change",
  "buffer_font_family": "JetBrainsMono Nerd Font",
  "confirm_quit": true,
  "relative_line_numbers": true,
  "features": {
    "edit_prediction_provider": "none"
  },
  "file_finder": {
    "modal_max_width": "large"
  },
  "format_on_save": "on",
  "formatter": "language_server",
  "icon_theme": "Catppuccin Mocha",
  "inlay_hints": {
    "enabled": false,
    "show_parameter_hints": false
  },
  "remove_trailing_whitespace_on_save": true,
  "scrollbar": {
    "show": "never"
  },
  "show_completion_documentation": true,
  "show_completions_on_input": true,
  "show_edit_predictions": false,
  "tab_bar": {
    "show": false
  },
  "restore_on_startup": "last_session",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "dark": "Nord Dark",
    "light": "Catppuccin Latte",
    "mode": "dark"
  },
  "ui_font_family": "JetBrainsMono Nerd Font",
  "use_auto_surround": true,
  "vim_mode": true,
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  },
  "languages": {
    "Templ": {
      "language_servers": [
        "templ",
        "tailwindcss-language-server",
        "vscode-html-language-server",
        "emmet-language-server"
      ]
    }
  },
  "lsp": {
    "tailwindcss-language-server": {
      "settings": {
        "includeLanguages": {
          "templ": "html"
        },
        "experimental": {
          "classRegex": [
            "class=\"([^\"]*)\"",
            "className=\"([^\"]*)\""
          ]
        }
      }
    }
  }
}
