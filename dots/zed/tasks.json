[
  {
    "label": "Run Go File",
    "command": "go run $ZED_FILE",
    // "use_new_terminal": true,
    "reveal": "always"
  },
  {
    "label": "Run Lua File",
    "command": "lua $ZED_FILE",
    // "use_new_terminal": true,
    "reveal": "always"
  },
  {
    "label": "Run Python File",
    "command": "python $ZED_FILE",
    "use_new_terminal": false,
    "reveal": "always"
  },
  {
    "label": "Run Typescript",
    "command": "bun run $ZED_FILE",
    // "use_new_terminal": true,
    "reveal": "always"
  },
  {
    "label": "File Finder",
    "command": "zeditor \"$(tv files)\"",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true
  },
  {
    "label": "Open Project",
    "command": "zeditor --add \"$(fd -t d . ~/coding/ | tv)\"",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true
  },
  {
    "label": "Update All Projects",
    "command": "gm -lg",
    "hide": "always",
    "allow_concurrent_runs": true,
    "use_new_terminal": true,
    "reveal_target": "center"
  }
]
